{
  "useTabs": true,
  "tabWidth": 2,
  "printWidth": 100,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "all",
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "lf",
  "ignorePatterns": [
    "node_modules",
    "target",
    "dist",
    "build",
    "**/generated/**",
    "**/*.d.ts"
  ]
}
